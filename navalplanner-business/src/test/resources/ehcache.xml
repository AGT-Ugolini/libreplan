<ehcache>
    <!-- DiskStore is commented out not to write to disk. Set
      the option overflowToDisk="false" required too
        <diskStore path="java.io.tmpdir"/>
    -->
    <defaultCache
        maxElementsInMemory="10000"
        eternal="false"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120"
        overflowToDisk="false"
    />
1</ehcache>