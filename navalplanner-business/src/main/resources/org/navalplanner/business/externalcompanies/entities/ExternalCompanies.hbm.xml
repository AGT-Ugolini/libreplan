<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.navalplanner.business.externalcompanies.entities" default-access="field">

<!-- ExternalCompany -->
<class name="ExternalCompany" table="EXTERNAL_COMPANY">
    <id name="id" type="long" access="property">
        <generator class="hilo">
            <param name="max_lo">100</param>
        </generator>
    </id>
    <version name="version" access="property" type="long" />

    <property name="name" unique="true" />

    <property name="nif" unique="true" />

    <property name="client" />

    <property name="subcontractor" />

    <property name="interactsWithApplications" />

    <property name="appURI" />

    <property name="ourCompanyLogin" />

    <property name="ourCompanyPassword" />
</class>

</hibernate-mapping>
